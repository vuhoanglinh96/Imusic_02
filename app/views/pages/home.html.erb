<% provide :title, full_title(t(".title")) %>

<body>
  <%= render "shared/swipe", track: @user_support.track,
    genres_map: @user_support.genres_map %>
  <div class="container contant">
    <div class="row">
      <div class="site-overlay"></div>
      <div class="col-md-12 basic">
        <%= render "shared/header" %>
        <%= render "shared/flash" %>
        <div class="place-li-cont">
          <h1><%= t ".welcome" %> <%= current_user.name %></h1>
          <p class="lead"><%= t ".enjoy" %></p>

          <div class="audio-players">
            <%= render "shared/track", track_index: @track_index %>
          </div>
        </div>
        <%= link_to t(".load"), "#", class: "more-btn", method: :get %>
      </div>
    </div>
  </div>
</body>
